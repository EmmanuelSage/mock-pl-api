{
  "name": "mock-pl-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "build": "rm -rf ./dist && babel -d ./dist ./src -s",
    "test": "NODE_ENV=test jest --runInBand --testTimeout=40000 --forceExit",
    "test:watch": "NODE_ENV=test jest --watchAll --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:coveralls": "NODE_ENV=test jest --coverage --testTimeout=40000 --coverageReporters=text-lcov --forceExit | coveralls"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmmanuelSage/mock-pl-api.git"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "Emmanuel Oluyale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmmanuelSage/mock-pl-api/issues"
  },
  "homepage": "https://github.com/EmmanuelSage/mock-pl-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@shelf/jest-mongodb": "^1.1.5",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.21",
    "redis": "^3.1.1"
  }
}
